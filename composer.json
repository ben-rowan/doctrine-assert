{
  "name": "ben-rowan/doctrine-assert",
  "description": "A collection of PHPUnit database assertions based on Doctrine2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Rowan"
    }
  ],
  "require": {
    "php": "^7.2",
    "phpunit/phpunit": "^7.1",
    "doctrine/orm": "^2.6",
    "symfony/finder": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "BenRowan\\DoctrineAssert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenRowan\\DoctrineAssert\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "ext-sqlite":  "*",
    "phpstan/phpstan": "^0.9.2",
    "phpmd/phpmd" : "@stable",
    "mikey179/vfsstream": "^1.6",
    "symfony/yaml": "^4.0",
    "fzaninotto/faker": "^1.7"
  }
}
